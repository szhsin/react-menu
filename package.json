{
  "name": "@szhsin/react-menu",
  "version": "1.2.0",
  "description": "An accessible, responsive, and customisable React menu library.",
  "author": "Zheng Song",
  "license": "MIT",
  "repository": "szhsin/react-menu",
  "homepage": "https://szhsin.github.io/react-menu/",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format cjs --css-modules false --sourcemap false",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --css-modules false --sourcemap false",
    "prepare": "run-s build",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage=true",
    "lint": "eslint .",
    "example": "npm start --prefix example",
    "predeploy": "cd example && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "prop-types": "^15.5.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^2.2.0",
    "jest": "^26.6.1",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^4.0.0",
    "react-test-renderer": "^16.14.0",
    "sass": "^1.26.10"
  },
  "dependencies": {},
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "menu",
    "submenu",
    "contextmenu",
    "dropdown",
    "checkbox",
    "radio button",
    "accessibility",
    "wai-aria"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/utils/"
    ]
  }
}