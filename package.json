{
  "name": "@szhsin/react-menu",
  "version": "1.6.0",
  "description": "React component for building accessible menu, dropdown, submenu, context menu and more.",
  "author": "Zheng Song",
  "license": "MIT",
  "repository": "szhsin/react-menu",
  "homepage": "https://szhsin.github.io/react-menu/",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npm run types && microbundle-crl --no-compress --format cjs --css-modules false --sourcemap false",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --css-modules false --sourcemap false",
    "prepare": "run-s build",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage=true",
    "lint": "eslint .",
    "dtslint": "dtslint --localTs node_modules/typescript/lib types",
    "types": "npm run dtslint && mkdir -p dist && cp -f types/index.d.ts dist/",
    "tsc": "cd types && npx tsc",
    "example": "npm start --prefix example",
    "predeploy": "cd example && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "prop-types": ">=15.7.2",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@types/estree": "^0.0.46",
    "@types/react": "^17.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "dtslint": "^4.0.6",
    "eslint": "^7.19.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^2.2.0",
    "jest": "^26.6.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.32.6",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "menu",
    "component",
    "submenu",
    "contextmenu",
    "dropdown",
    "checkbox",
    "radio button",
    "accessibility",
    "wai-aria"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/utils/"
    ]
  }
}
