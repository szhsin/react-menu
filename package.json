{
  "name": "@szhsin/react-menu",
  "version": "1.10.1",
  "description": "React component for building accessible menu, dropdown, submenu, context menu and more.",
  "author": "Zheng Song",
  "license": "MIT",
  "repository": "szhsin/react-menu",
  "homepage": "https://szhsin.github.io/react-menu/",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "bundle": "microbundle-crl --no-compress --css-modules false --sourcemap false",
    "watch": "microbundle-crl watch --no-compress --format modern,cjs --css-modules false --sourcemap false",
    "clean": "rm -rf dist && mkdir -p dist",
    "start": "run-p \"css -- --watch\" watch",
    "build": "run-s clean lint types css \"bundle -- --format modern\" \"bundle -- --format cjs\"",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage=true",
    "lint": "eslint .",
    "dtslint": "dtslint --localTs node_modules/typescript/lib types",
    "css": "sass src/styles:dist --no-source-map",
    "types": "npm run dtslint && cp -f types/index.d.ts dist/",
    "tsc": "cd types && npx tsc",
    "example": "npm start --prefix example",
    "predeploy": "cd example && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/estree": "^0.0.48",
    "@types/react": "^17.0.14",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "cross-env": "^7.0.3",
    "dtslint": "^4.1.1",
    "eslint": "^7.30.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^2.2.0",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.0.6",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.35.1",
    "typescript": "^4.3.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "menu",
    "component",
    "submenu",
    "contextmenu",
    "dropdown",
    "checkbox",
    "radio button",
    "accessibility",
    "wai-aria"
  ]
}
